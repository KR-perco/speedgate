    .container
        .row
            .cell-3.cell-0-md
            .cell-6.cell-12-md
                .icons_list.characteristics.row
                    each object, key in data.home.characteristics.icons
                        .icon__item.cell-2
                            figure.icon(
                                class=object.svg 
