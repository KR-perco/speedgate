//- скоростной проход st-11
section.ver1.video1#prod-2 
    .container
        .row 
            .js-video-main-wrap.push-2.cell-8.post-2.push-0-md.cell-12-md.post-0-md
                .header-demo.flex-between.flex-middle(data-aos="fade-in" data-aos-anchor-placement="center-center")
                    h2.header-demo__text Скоростной проход ST-11
                    a.btn.header-demo__video-btn(data-fancybox-plyr data-src=data.home.ytLink.st11 mainClass="full-vid") смотреть видео
                .btn.js-startstop Stop
                .swiper-container.slider-demonstration.js-sliderdemo-2(data-aos="fade-in" data-aos-anchor-placement="top-center")
                    .swiper-wrapper
                        .swiper-slide(data-name="Ракурсы")
                            +lazyVideo({
                                poster: 'images/video-posters/poster1.jpg',
                                source: [
                                    {
                                        srcset: `video/st11/990x560/1.mp4`,
                                        type: 'video/mp4',
                                    }, 
                                    { 
                                        srcset: `video/st02/740x418/1.mp4`,
                                        type: 'video/mp4', 
                                        media: "all and (max-width: 740px)",
                                        dataMw: "740"
                                    }
                                ]
                            }).lozad#index-1(autoplay, muted)
                        .swiper-slide(data-name="Проход")
                            +lazyVideo({
                                poster: 'images/video-posters/poster2.jpg',
                                source: [
                                    {
                                        srcset: `video/st11/990x560/2.mp4`, 
                                        type: 'video/mp4',
                                    },
                                    { 
                                        srcset: `video/st02/740x418/1.mp4`,
                                        type: 'video/mp4', 
                                        media: "all and (max-width: 740px)",
                                        dataMw: "740"
                                    }
                                ]
                            }).lozad#index-2(muted)
                        .swiper-slide(data-name="Габариты") 
                            +lazyVideo({
                                poster: 'images/video-posters/poster3.jpg',
