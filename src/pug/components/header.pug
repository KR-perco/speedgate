		nav.header__nav-container.hide-s
			ul.nav-container
				each object, key in data.header.menu
					li.nav-container__item
						a.nav-container__link(
							href=object.url
							data-link=key
						)
							!= object.name 
						if object.sublist
							ul.nav-container__sublist.nav-sublist
								each subobject, key in object.sublist
									li
										a.nav-sublist__link(
											href=subobject.url
											data-link=key
										)
											.nav-sublist__img
												img(src=subobject.img)
											span.nav-sublist__span
												!= subobject.name 
		
		.header__language
			.select-lang
				span.select-lang__name EN
				ul.select-lang__list 
					li
						a(href="#").select-lang__item.select-lang__item_active English
					li
						a(href="#").select-lang__item Italian

		a.header__pen(href=data.header.links.writeus data-aos="fade-in" data-aos-easing="ease-in-out" data-aos-anchor-placement="top-center" data-aos-delay="700" data-aos-duration="1000")
			.header__pen-img

